/* 底部规则 */
.listRule {
    position: relative;
    bottom: 0;
    z-index: 2;
    width: 700rpx;
    height: 320rpx;
    left: 25rpx;
}
.listRule-background {
    position: absolute;
    bottom: 0;
    display: block;
    width: 700rpx;
    height: 320rpx;
}
.rule-title {
    position: relative;
    z-index: 3;
    padding-top: 40rpx;
}
.rule-title-txt {
    font-size: 32rpx;
    line-height: 32rpx;
    text-align: center;
    color: #fff;
}
.rule-title-bottom {
    display: block;
    margin: 7rpx auto 0;
    width: 231rpx;
    height: 13rpx;
}

.rule-detail {
    position: relative;
    z-index: 3;
    font-size: 26rpx;
    color: #a594e4;
    text-indent: 2em;
    padding: 30rpx 45rpx 0 50rpx;
}
/* 获取授权弹框 */
.getUserInfoBtn {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background: rgba(0, 0, 0, 0.4);
    z-index: 4;
}
.getUserInfo {
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    box-shadow: none;
    border: none;
    background: rgba(0, 0, 0, 0.3);
}
.getInfoTxt {
    font-size: 32rpx;
    color: #5c1166;
    text-align: center;
    position: relative;
    top: 218rpx;
    letter-spacing: -2rpx;
}
.getInfoBtn {
    position: absolute;
    bottom: 40rpx;
    left: 50%;
    transform: translate(-50%, 0);
    z-index: 10;
    width: 374rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    background: linear-gradient(to right, #e43acf, #713bf0);
    color: #fff;
    font-size: 35rpx;
    border-radius: 40rpx;
}
.getUserInfoWrapper {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 644rpx;
    height: 425rpx;
}
.getUserInfoBg {
    position: absolute;
    width: 100%;
    height: 100%;
}
