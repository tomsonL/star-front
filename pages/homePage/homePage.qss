.homePage {
}
.bg {
    position: fixed;
    width: 750rpx;
    height: 100vh;
}
.searchBtn {
    position: relative;
    top: 21rpx;
    margin: 0 auto;
    background: rgba(238, 238, 238, 0.1);
    border: 1p solid #fff;
    border-radius: 27rpx;
    width: 696rpx;
    height: 53rpx;
    text-align: center;
    font-size: 28rpx;
    color: rgba(255, 255, 255, 0.6);
    line-height: 53rpx;
}
.searchIcon {
    display: inline-block;
    width: 25rpx;
    height: 25rpx;
    margin-right: 15rpx;
    vertical-align: middle;
}
/* 头部 */
.header {
    padding-top: 20rpx;
}
.headerImg {
    position: relative;
    z-index: 10;
    display: block;
    margin: 0 auto;
    width: 740rpx;
    height: 211rpx;
}
.headerBtn {
    width: 140rpx;
    height: 60rpx;
    line-height: 60rpx;
    border-radius: 30rpx;
}
/* 控制星座 */
.switch-ctrl {
    margin-top: 66rpx;
}
.swiperShadow {
    position: absolute;
    width: 100%;
    height: 65rpx;
    background: rgba(0, 0, 0, 0);
    z-index: 10;
}
.switch-ctrl swiper {
    height: 65rpx;
    line-height: 58rpx;
}
.switch-ctrl swiper-item {
}
.switch-ctrl swiper-item .ctrl-name {
    text-align: center;
    font-size: 28rpx;
    color: #7f7e7f;
}
.switch-ctrl swiper-item .active {
    color: #fff;
    font-size: 34rpx;
    border-bottom: 1px solid #fff;
}
/* body轮盘 */
.body {
    position: relative;
}
.rule-wrapper {
    position: absolute;
    top: 10rpx;
    right: 13rpx;
    width: 139rpx;
    height: 40rpx;
    line-height: 40rpx;
    background: rgba(0, 0, 0, 0.4);
    border-radius: 20rpx;
    padding-left: 23rpx;
    color: #fff800;
    font-size: 24rpx;
    z-index: 3;
}
.rule-icon {
    position: absolute;
    right: 17rpx;
    top: 9rpx;
    display: block;
    width: 21rpx;
    height: 22rpx;
}
/* 星盘 */
.horoscopeWrapper {
    margin: 35rpx auto;
    width: 750rpx;
    height: 781rpx;
}
.horoBg {
    position: absolute;
    width: 750rpx;
    height: 781rpx;
}
.totalTop {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    width: 180rpx;
    height: 180rpx;
    border-radius: 50%;
}
.totalTitle {
    position: absolute;
    width: 193rpx;
    height: 73rpx;
    left: -5rpx;
    top: -15rpx;
    transform: translate(0, -25%);
    z-index: 3;
}
.totalBg {
    position: absolute;
    width: 100%;
    height: 100%;
    border-radius: 50%;
    z-index: 2;
}
.totalAvatar {
    position: absolute;
    top: 4%;
    left: 4%;
    border-radius: 50%;
    width: 90%;
    height: 90%;
    border: 1px solid rgba(112, 112, 112, 0.99);
}
.icon_wrapper {
    position: absolute;
}
.icon_wrapper image {
    position: absolute;
    width: 100%;
    height: 100%;
}
